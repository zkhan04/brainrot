{
    "manifest_version":3,
    "version":"1.0",
    "name":"brainrot virus",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"],
            "type": "module"
        }
    ],
    "action": {
        "default_popup": "shop/shop.html",
        "default_icon": "assets/mango.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"],
    "web_accessible_resources": [
        {
            "resources": [
                "assets/mango.png",
                "assets/hawktuahTrim.mp3",
                "assets/iunderstanditnow.mp3",
                "assets/mangos.mp3",
                "assets/aliens.mp3",
                "assets/mulch1.png",
                "assets/mulch2.jpeg",
                "assets/mulch.mp3",
                "assets/Costco.jpeg",
                "assets/costcoGuys.mp3",
                "assets/therizzler.gif",
                "assets/balkanRage.jpeg",
                "assets/justlostmydawg.mp3",
                "assets/vine.mp3",
                "assets/thickofit.mp3",
                "assets/lunchly.jpeg",
                "assets/subway_surfers.mp4",
                "style.css"
            ],
            "matches": ["<all_urls>"]
        }
    ],
    "permissions": ["storage", "alarms"]
  }